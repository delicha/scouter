<% if @user.sub_images.attached? %>
  <p>
    <i class="fa-solid fa-image-portrait fa-3x mt-5 text-brown"></i>
  </p>
  <h3 class="text-brown my-3 py-3">- その他画像 -</h3>
  <% @user.sub_images.each do |attachment| %>
    <%= image_tag attachment.variant(resize: "400x400"), class: 'rounded border mx-2' %>
    
  <% end %>
<% end %>
